pipeline{
    agent any

    stages{
        stage("A"){
            steps{
                echo "========executing A========"
            }
            post{
                always{
                    echo "========always========"
                }
                success{
                    echo "========A exgutecuted successfully========"
                }
                failure{
                    echo "========A execution failed========"
                }
            }
        }
    }
   
        }
    
